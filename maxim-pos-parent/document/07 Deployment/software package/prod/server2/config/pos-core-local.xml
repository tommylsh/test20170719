<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
          http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd
 ">

	<util:map id="masterFolderConfiguration" 
	map-class="java.util.HashMap"
	key-type="java.lang.String"
    value-type="com.maxim.pos.common.entity.BranchInfo">
				<entry key="FILE_MIDPOS" value-ref="midposInfo"/>
				<entry key="FILE_POINTSOFT" value-ref="pointsoftInfo"/>
	</util:map>
	
	<util:map id="masterFolderFilter" 
	map-class="java.util.HashMap"
	key-type="java.lang.String"
    value-type="java.lang.String">
				<entry key="FILE_MIDPOS" value=""/>
				<entry key="FILE_POINTSOFT" value=""/>
	</util:map>

	<bean id="midposInfo" class="com.maxim.pos.common.entity.BranchInfo">
		<property name="clientHost" value="10.20.30.166"/>
		<property name="clientDB" value="export/{BRANCH_CODE}"/>
		<property name="user" value="poll"/>
		<property name="password" value="poll12345"/>
		<property name="enable" value="true"/>
	</bean>
	
	<bean id="pointsoftInfo" class="com.maxim.pos.common.entity.BranchInfo">
		<property name="clientHost" value="10.20.30.166"/>
		<property name="clientDB" value="export"/>
		<property name="user" value="poll"/>
		<property name="password" value="poll12345"/>
		<property name="enable" value="true"/>
	</bean>
	
	
	<util:map id="filecopySourceFolderConfiguration" 
	map-class="java.util.HashMap"
	key-type="java.lang.String"
    value-type="com.maxim.pos.common.entity.BranchInfo">
				<entry key="OCT_JCR" value-ref="octJcrPathInfo"/>
				<entry key="OCT_AROME" value-ref="octAromePathInfo"/>
				<entry key="OCT_OTHER" value-ref="octOtherPathInfo"/>
	</util:map>
	
	<util:map id="filecopyTargetFolderConfiguration" 
	map-class="java.util.HashMap"
	key-type="java.lang.String"
    value-type="java.util.ArrayList">
				<entry key="OCT_JCR" value-ref="octJcrTarget"/>
				<entry key="OCT_AROME" value-ref="octAromeTarget"/>
				<entry key="OCT_OTHER" value-ref="octOtherTarget"/>
	</util:map>
	
	<util:map id="filecopyTargetBranchConfiguration" 
	map-class="java.util.HashMap"
	key-type="java.lang.String"
    value-type="java.util.ArrayList">
				<entry key="OCT_JCR" value-ref="octJcrBranchList"/>
				<entry key="OCT_AROME" value-ref="octAromeBranchList"/>
				<entry key="OCT_OTHER" value-ref="octOtherBranchList"/>
	</util:map>
	
