package com.maxim.pos.common.enumeration;

public class MasterSyncType {

    private Type type;

    private String value;

    public enum Type {CODE, TYPE, ALL}

    public MasterSyncType() {}

    public MasterSyncType(Type type, String value) {
        this.type = type;
        this.value = value;
    }

    public Type getType() {
        return type;
    }

    public void setType(Type type) {
        this.type = type;
    }

    public String getValue() {
        return value;
    }

    public void setValue(String value) {
        this.value = value;
    }

}
